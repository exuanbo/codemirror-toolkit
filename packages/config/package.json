{
  "name": "@codemirror-toolkit/config",
  "private": true,
  "exports": {
    "./*": {
      "require": {
        "types": "./*.d.ts",
        "default": "./*.js"
      },
      "import": {
        "types": "./*.d.mts",
        "default": "./*.mjs"
      }
    },
    "./*.js": {
      "types": "./*.d.ts",
      "default": "./*.js"
    },
    "./*.mjs": {
      "types": "./*.d.mts",
      "default": "./*.mjs"
    },
    "./*.json": "./*.json"
  }
}
